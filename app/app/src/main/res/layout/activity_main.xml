<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.lu.che.MainActivity">

    <WebView
        android:id="@+id/web"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <Button
        android:id="@+id/h"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_below="@+id/y"
        android:layout_gravity="bottom"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="5dp"
        android:layout_toEndOf="@+id/z"
        android:layout_toRightOf="@+id/z"
        android:background="@drawable/button"
        android:text="后" />

    <Button
        android:id="@+id/z"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/q"
        android:layout_gravity="bottom"
        android:layout_marginTop="5dp"
        android:background="@drawable/button"
        android:text="左" />

    <Button
        android:id="@+id/y"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignTop="@+id/s"
        android:layout_gravity="bottom"
        android:layout_marginLeft="5dp"
        android:layout_toEndOf="@+id/s"
        android:layout_toRightOf="@+id/s"
        android:background="@drawable/button"
        android:text="右" />

    <Button
        android:id="@+id/q"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_above="@+id/s"
        android:layout_alignLeft="@+id/s"
        android:layout_alignStart="@+id/s"
        android:background="@drawable/button"
        android:text="前" />

    <Button
        android:id="@+id/s"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="55dp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="5dp"
        android:layout_toEndOf="@+id/z"
        android:layout_toRightOf="@+id/z"
        android:background="@drawable/button"
        android:text="停" />

</RelativeLayout>
